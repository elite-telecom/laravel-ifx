{
    "name": "elite-telecom/laravel-ifx",
    "description": "Informix DB driver for Laravel 5",
    "keywords": ["informix", "laravel", "laravel 5", "ifx"],
    "license": "MIT",
    "version": "1.0.5",
    "authors": [{
        "name": "tabusheikh",
        "email": "tabusheikh@networkip.net"
    }],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.8.*",
        "illuminate/database": "5.8.*",
        "illuminate/pagination": "5.8.*"
    },
    "require-dev": {
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.2",
        "phpunit/phpunit": "8.0.4",
        "phpspec/phpspec": "5.1.0"
    },
    "autoload": {
        "psr-4": {
            "EliteTelecom\\": "src/elite-telecom"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "EliteTelecom\\Informix\\InformixDBServiceProvider"
            ]
        }
    }
}
